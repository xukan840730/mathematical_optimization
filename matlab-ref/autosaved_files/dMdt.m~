function Mdot = carb_odes(t,M) 
global k

F=zeros(9,9);
for n=1:9;
  F(n,:)=k(n,:).*M(n,1);
end
% Mdot(1,1)=(F(2,1)+F(5,1)+F(7,1)+F(8,1)+F(9,1)) -(F(1,2)+F(1,5));
% Mdot(2,1)=(F(1,2)+F(3,2)+F(4,2)+F(7,2))             -(F(2,1)+F(2,3)+F(2,4));
% Mdot(3,1)=(F(2,3))                                                  -(F(3,2)+F(3,4));
% Mdot(4,1)=(F(2,4)+F(3,4))                                     -(F(4,2));
% Mdot(5,1)=(F(1,5))                                                  -(F(5,1)+F(5,6)+F(5,7));
% Mdot(6,1)=(F(5,6))                                                  -(F(6,7));
% Mdot(7,1)=(F(5,7)+F(6,7))                                      -(F(7,1)+F(7,2)+F(7,8)+F(7,9));
% Mdot(8,1)=(F(7,8))                                                 -(F(8,1));
% Mdot(9,1)=(F(7,9))                                                  -(F(9,1));
